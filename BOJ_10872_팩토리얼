import java.util.Scanner;

public class BOJ_10872 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int sum = fac(N);
		System.out.println(sum);
		
		
		
	}

	private static int fac(int N) {
		if(N<=1) return 1;  //0!ì€ 1
		
		return N * fac(N-1);
	}
	
	
	
}
